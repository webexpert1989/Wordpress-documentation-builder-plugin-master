[[header]]

<div class = "doc-menu-wrapper">
	<div class = "doc-logo-wrapper">
		<div class = "doc-logo"><img src = "{{doc_logo}}"/></div>
	</div>

	<div id = "doc-menus" class = "doc-menus">[[menu]]</div>
</div>


<div class = "doc-content-wrapper">

	<div class = "doc-header">
		<div class = "doc-title">{{doc_name}}</div>
		<div class = "doc-description mceContentBody clear-after">{{doc_desc}}</div>
	</div>

	<div class = "doc-content">
		<div class = "doc-chapters">[[contents]]</div>
	</div>

</div>

[[footer]]