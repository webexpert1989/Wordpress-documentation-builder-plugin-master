[[header]]

<div class = "doc-header clear-after">
	<div class = "doc-logo"><img src = "{{doc_logo}}"/></div>
	<div class = "doc-title">{{doc_name}}</div>
	<div class = "doc-description mceContentBody clear-after">{{doc_desc}}</div>
</div>

<input type = "hidden" id = "doc-created-date" value = "{{doc_date}}"/>
<input type = "hidden" id = "doc-created-author" value = "{{doc_author}}"/>

<div class = "doc-content-wrapper clear-after">
	<div id = "doc-menus-top"></div>
	<div id = "doc-menus" class = "doc-menus">[[menu]]</div>
	<div class = "doc-content"><div class = "doc-chapters">[[contents]]</div></div>
</div>

[[footer]]